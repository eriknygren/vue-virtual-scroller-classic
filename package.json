{
  "name": "vue-virtual-scroller-classic",
  "description": "A Vue 3 implementation of the original VirtualScroller from vue-virtual-scroller",
  "version": "1.1.2",
  "author": {
    "name": "Erik Nygren",
    "email": "hello@eriknygren.dev"
  },
  "keywords": [
    "vue",
    "vuejs",
    "plugin"
  ],
  "license": "MIT",
  "main": "dist/vue-virtual-scroller-classic.umd.js",
  "module": "dist/vue-virtual-scroller-classic.esm.js",
  "unpkg": "dist/vue-virtual-scroller-classic.min.js",
  "scripts": {
    "build": "npm run build:browser && npm run build:es && npm run build:umd",
    "build:browser": "rollup --config build/rollup.config.browser.js",
    "build:es": "rollup --config build/rollup.config.es.js",
    "build:umd": "rollup --config build/rollup.config.umd.js",
    "prepublishOnly": "npm run lint && npm run build",
    "dev": "cross-env NODE_ENV=development rollup --config build/rollup.config.es.js --watch",
    "lint": "eslint --ext .js,.vue src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eriknygren/vue-virtual-scroller-classic.git"
  },
  "bugs": {
    "url": "https://github.com/eriknygren/vue-virtual-scroller-classic/issues"
  },
  "homepage": "https://github.com/eriknygren/vue-virtual-scroller-classic#readme",
  "peerDependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.9.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-replace": "^2.3.1",
    "@vue/compiler-sfc": "^3.0.11",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.1.0",
    "clean-css": "^4.1.8",
    "cross-env": "^7.0.2",
    "eslint": "^7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-vue": "^7.9.0",
    "fs-extra": "^9.1.0",
    "rollup": "^2.45.2",
    "rollup-plugin-css-only": "^2.0.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.2.41"
  },
  "dependencies": {
    "scrollparent": "^2.0.1",
    "vue-observe-visibility": "Akryum/vue-observe-visibility#c539a011090e33c685719a3af5e80428f252f8a0",
    "vue-resize": "^2.0.0-alpha.1"
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
